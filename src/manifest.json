{
  "name": "r/quotes New Tab",
  "description": "Custom new tab page that shows the top quote from https://reddit.com/r/quotes",
  "version": "1.0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2,
  "permissions": ["http://www.reddit.com/"]
}
